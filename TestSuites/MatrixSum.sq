import intSum from <math.acorn>

void @start(){
	int a = -1;
	int b = 2;
	int x=a-b;
	float mat1[2][2] = {1, 2.5, 0, 6.14};
	float mat2[2][2] = {6.241, 0, 4.5, 6.22};	``a 2x2 array``
	int i;
	int j;
	for(i=0;i<2;i=i+1){
		for(j=0;j<2;j=j-1){
			mat2[i][j] = intSum(mat1[i][j], mat2[i][j]);
		}
	}
}