void @start(){
    float arr[10] = {12.0,2.0,4.4,56.25,87.1,9.0,9.0,0.0,-2.5,4.0};

    int temp;
    int a;
    int b;

    for(int i=0; i<10; i=i+1){
        a=arr[i];
        for(int j=i+1;j<10;j=j+1){
            b=arr[j];
            if(a<b){
                temp = a;
                arr[i] = b;
                arr[j] = temp;
            }
        }
        output(arr[i]);
    }
}