int @fibonacci (int n)
{
    if (n == 0)
    {
        return 0;
    }
    elif (n == 1)
    {
        return 1;
    } 
    else{
        int r = fibonacci(n-1) + fibonacci(n-2); 
        return r;
    }   
        
}
void @start() {
	int n = 12;
	int a = fibonacci(n);
    output(a);
    output("\n");

}
