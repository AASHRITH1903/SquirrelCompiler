int @fibonacci (int n)
{
    if (n == 0)
    {
        return 0;
    }
    elif (n == 1)
    {
        return 1;
    } 
    else{
        int r = fibonacci(n-1) + fibonacci(n-2); 
        return r;
    }   
        
}
void @start() {
	

    for(int i=0; i<10; i=i+1){
	    int a = fibonacci(i);
        output(a);
        output("\n");
    }
