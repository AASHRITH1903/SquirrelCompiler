`` fibonacci iterative ``
int @fibonacci(int n) {
    int a = 0;
    int b = 1;
    int c = 0;
    for (int i = 0; i < n; i = i + 1) {
        c = a + b;
        a = b;
        b = c;
    }

    return c;
}

void @start() {
    output("Fibonacci iterative\n");

    int n;
    output("Enter a number: ");

    input(n);
    int res = fibonacci(n);

    output("Fibonacci of ");
    output(n);
    output(" is ");
    output(res);
    output("\n");
}