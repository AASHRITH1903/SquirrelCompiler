int @gcd_recursive(int a, int b) {
    if (b == 0) {
        return a;
    }
    return gcd_recursive(b, a % b);
}

void @start() {
    int a;
    output("Enter the first number: ");
    input(a);
    
    int b;
    output("Enter the second number: ");
    input(b);

    int gcd = gcd_recursive(a, b);
    output("The GCD of ");
    output(a);
    output(" and ");
    output(b);
    output(" is ");
    output(gcd);
    output("\n");
}